{
  "name": "@lewin671/python-vm",
  "version": "0.1.4",
  "description": "A powerful, high-performance Python 3 Virtual Machine and Compiler engineered in pure TypeScript.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "README_zh-CN.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "PYTHONHASHSEED=0 vitest run",
    "test:watch": "PYTHONHASHSEED=0 vitest",
    "test:coverage": "PYTHONHASHSEED=0 vitest run --coverage",
    "benchmark": "npm run build && node scripts/benchmark.cjs",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "postpublish": "bash scripts/release.sh"
  },
  "keywords": [
    "python",
    "compiler",
    "interpreter",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
